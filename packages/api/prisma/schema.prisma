generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // url      = env("DATABASE_URL")
  url = "postgresql://ziqiln:ziqiln@postgres:5432/mydb?schema=sandbox"
}

model User {
  id          Int      @default(autoincrement()) @id
  email       String   @unique
  firstName   String?
  lastName    String?
  password    String?
  role        Role     @default(dbgenerated())
}

enum Role {
  ADMIN
  USER
}
